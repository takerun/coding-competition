{
    "editor.tabCompletion": "on",
    "editor.formatOnSave": true,
    "files.associations": {
        "iostream": "cpp",
        "random": "cpp"
    },
    "code-runner.runInTerminal": true,
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt.out -fdiagnostics-color=always -std=c++14 -I $workspaceRoot/lib/ac-library && $dir$fileNameWithoutExt.out",
    },
}