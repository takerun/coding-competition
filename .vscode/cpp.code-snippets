            "        int chi = tree[node][i];",
            "        if(chi == par) continue;",
            "        score_subtree[node] += score_subtree[chi];",
            "    }",
            "    // int64 score_partree = socre_wholetree - score_subtree[node];",
            "    return 0LL;",
